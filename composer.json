{
    "name": "tripup-company/laravel-responsecache-tags",
    "description": "Custom taged cache implementation",
    "type": "library",
    "require": {
        "laravel/framework": "*",
        "spatie/laravel-responsecache": "^6.6",
        "spatie/laravel-package-tools": "^1.1",
        "spatie/data-transfer-object": "^1.13|^2.8",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.x-dev",
        "orchestra/testbench": "6.x-dev"
    },
    "license": "GPL",
    "authors": [
        {
            "name": "A.Lisnyak",
            "email": "andrey.lisnyak@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "TripUp\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TripUp\\Cache\\TagCacheServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
